num_1 = int(input("Proporciona la base: "))
num_2 = int(input("Proporciona la altura: "))


class Rectangulo:
    def __init__(self, base, altura):
        self.base = base
        self.altura = altura

    def calcular_area(self):
        return self.base * self.altura


rectangulo_1 = Rectangulo(num_1, num_2)
print(f"Área rectángulo: {rectangulo_1.calcular_area()}")
